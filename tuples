def multiply_tuple_elements(numbers):
    product = 1
    for num in numbers:
        product *= num
    return product

# Example tuple
my_tuple = (2, 3, 4, 5)

# Calculate product
result = multiply_tuple_elements(my_tuple)

print("Product of tuple elements:", result)
rom functools import reduce

my_tuple = (2, 3, 4, 5)
product = reduce(lambda x, y: x * y, my_tuple)

print("Product using reduce:", product)
